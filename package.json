{
  "name": "june-coding-event",
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "test": "jest"
  },
  "dependencies": {
    "@material-ui/core": "4.0.1",
    "@testing-library/react": "^8.0.1",
    "babel-polyfill": "^6.26.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "jest-dom": "^3.4.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-flex-material": "1.1.0",
    "react-test-renderer": "^16.8.6",
    "shortid": "2.2.14"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "jest-cli": "^24.8.0",
    "parcel-bundler": "^1.6.1"
  },
  "keywords": []
}
